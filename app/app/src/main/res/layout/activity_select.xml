<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="com.example.dimitrivc.final_project.SelectActivity">

    <Button
        android:id="@+id/button21"
        android:layout_width="100dp"
        android:layout_height="55dp"
        android:layout_weight="1"
        android:onClick="goToLoginSelect"
        android:text="Log out"
        android:gravity="top" />

    <TextView
        android:id="@+id/textViewTop"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/button21"/>

    <ListView
        android:id="@+id/listViewTop"
        android:layout_width="match_parent"
        android:layout_height="230dp"
        android:focusable="false"
        android:layout_below="@+id/textViewTop">
    </ListView>

    <TextView
        android:id="@+id/textViewBottom"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/listViewTop"/>

    <ListView
        android:layout_width="match_parent"
        android:layout_height="230dp"
        android:id="@+id/listViewBottom"
        android:layout_below="@+id/textViewBottom"
        />


</RelativeLayout>
